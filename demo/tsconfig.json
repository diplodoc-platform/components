{
  "extends": "@yandex-cloud/tsconfig/tsconfig",
  "compilerOptions": {
    "module": "esNext",
    "moduleResolution": "nodenext",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "declaration": true,
    "jsx": "react",
    "outDir": "build",
    "baseUrl": "."
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
